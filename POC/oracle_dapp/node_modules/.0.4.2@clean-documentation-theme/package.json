{
  "name": "clean-documentation-theme",
  "version": "0.4.2",
  "description": "Theme for documentation.js",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run build && aegir-test --env node",
    "lint": "aegir-lint",
    "build": "babel src -d lib",
    "dev": "babel src -d lib --watch",
    "release": "npm run build && aegir-release --env node",
    "release-minor": "npm run build && aegir-release --env node --type minor",
    "release-major": "npm run build && aegir-release --env node --type major",
    "coverage": "nyc npm test",
    "coverage-publish": "aegir-coverage publish"
  },
  "keywords": [
    "doc",
    "docs",
    "documentation",
    "documentation.js",
    "jsdoc",
    "jsdoc3",
    "theme"
  ],
  "author": "Friedel Ziegelmayer <dignifiedquire@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-transform-inline-imports-commonjs": "^1.2.0",
    "concat-stream": "^1.5.2",
    "documentation": "^4.0.0-beta14",
    "github-slugger": "^1.1.1",
    "highlight.js": "^9.8.0",
    "lodash": "^4.17.2",
    "radium": "^0.18.1",
    "radium-bootstrap-grid": "^0.1.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-icons": "^2.2.1",
    "react-pure-render": "^1.0.2",
    "vinyl": "^2.0.1",
    "vinyl-fs": "^2.4.4"
  },
  "devDependencies": {
    "aegir": "^9.1.2",
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "enzyme": "^2.6.0",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-react": "^6.7.1",
    "nyc": "^10.0.0",
    "react-addons-test-utils": "^15.4.1",
    "remark": "^6.2.0"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dignifiedquire/clean-documentation-theme.git"
  },
  "bugs": {
    "url": "https://github.com/dignifiedquire/clean-documentation-theme/issues"
  },
  "homepage": "https://github.com/dignifiedquire/clean-documentation-theme",
  "contributors": [
    "Friedel Ziegelmayer <dignifiedquire@gmail.com>"
  ],
  "_from": "clean-documentation-theme@0.4.2",
  "_resolved": "http://registry.npm.taobao.org/clean-documentation-theme/download/clean-documentation-theme-0.4.2.tgz"
}